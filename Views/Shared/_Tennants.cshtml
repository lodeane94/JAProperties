@model  IEnumerable<SS.Models.Tennant>
<div class="row">
    <div class="col-xs-12" id="accordion">
        @{
            foreach (var t in Model)
            {
                <h5 id="tennant-name"><span class="glyphicon glyphicon-user img-circle img-circle-sm" style="font-size:25px;"></span> @t.User.FirstName @t.User.LastName</h5>
                <div class="row">
                    <div class="col-xs-12">
                        <label>Non-active information can not be edited</label>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-6">
                        <label>First Name : @t.User.LastName</label>
                    </div>
                    <div class="col-md-offset-6 col-xs-6">
                        <label>Cell Number : @t.User.CellNum</label>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-6">
                        <label>Middle Name : @t.User.LastName</label>
                    </div>
                    <div class="col-md-offset-6 col-xs-6">
                        <label>Email : @t.User.Email</label>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-6">
                        <label>Last Name : @t.User.LastName</label>
                    </div>
                    <div class="col-md-offset-6 col-xs-6">
                        <label>DOB : @t.User.DOB</label>
                    </div>
                </div>
                <hr />
                <div class="row">
                    <div class="col-xs-6">
                        <label>Monthly Rent Amount : </label>
                        $ <div class="input-group">
                            <input id="price" value="@t.RentAmt" type="text" class="form-control">
                            <span class="input-group-addon glyphicon-color"><i class="glyphicon glyphicon-usd"></i></span>
                        </div>
                    </div>
                    <div class="col-md-offset-6 col-xs-6">
                        <label>Last Paid Rent : -</label>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-6">
                        <label>Institution Name : @t.User.Email</label>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-6">
                        <label>Settlement Period : @t.SettlementPeriod Years</label>
                    </div>
                    <div class="col-md-offset-6 col-xs-6">
                        <label>Institution Name : @t.InstitutionName</label>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-6">
                        <label>Programme Name : @t.ProgrammeName</label>
                    </div>
                    <div class="col-md-offset-6 col-xs-6">
                        <label>Programme Start Date : @t.ProgrammeStartDate</label>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-6">
                        <label>Programme End Date : @t.ProgrammeEndDate</label>
                    </div>
                    <div class="col-md-offset-6 col-xs-6">
                        <label>Enrolment Date : @t.DateTCreated</label>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-3">
                        <button class="btn btn-accent">Unenroll Tennant</button>
                    </div>
                    <div class="col-xs-3">
                        <button class="btn btn-accent">Unenroll Tennant</button>
                    </div>
                </div>
                <input type="hidden" id="tennant">
            }
        }
    </div>
</div>
